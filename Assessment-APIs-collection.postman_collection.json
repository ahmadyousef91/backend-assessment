      "item": [
        {
          "name": "login",
          "event": [
            {
              "listen": "test",
              "script": {
                "exec": [
                  "pm.test(\"Status is 200\", () => {",
                  "  pm.response.to.have.status(200);",
                  "});",
                  "",
                  "const json = pm.response.json();",
                  "",
                  "pm.test(\"accessToken exists\", () => {",
                  "  pm.expect(json.accessToken).to.be.a(\"string\").and.not.empty;",
                  "});",
                  "",
                  "// Save token for parent auth usage (Collection/Folder Authorization: Bearer {{accessToken}})",
                  "pm.collectionVariables.set(\"accessToken\", json.accessToken);",
                  "",
                  "// Optional: save full Authorization header value if you prefer {{authHeader}}",
                  "const tokenType = (json.tokenType || \"Bearer\").trim();",
                  "pm.collectionVariables.set(\"authHeader\", `${tokenType} ${json.accessToken}`);",
                  "",
                  "// Optional: save expiry",
                  "if (json.expiresInSeconds != null) {",
                  "  pm.collectionVariables.set(\"accessTokenExpiresInSeconds\", String(json.expiresInSeconds));",
                  "}",
                  ""
                ],
                "type": "text/javascript",
                "packages": {},
                "requests": {}
              }
            }
